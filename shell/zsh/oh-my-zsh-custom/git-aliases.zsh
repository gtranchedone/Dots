alias gb="git branch"
alias gc="git commit -m"
alias gco="git checkout"
alias gca="git add --all && gc"
alias gcap="git add --all && gc $1 && gp"
alias gf="git fetch --prune"
alias gp="git push"
alias gpl="git pull"
alias gplr="gpl --rebase"
alias gs="git status"
alias glog="git log --graph --oneline"
alias gsu="git submodule update --recursive --init"
alias gtd="git push --delete origin"
